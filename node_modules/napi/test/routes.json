{
  "defaults": {
    "params": {
    
      "user": {
        "type": "string"
      }
      
    }
  },
  
  "name": "github",
  
  "methods": {
    "user": {
      "return": "github.user",
      "private": {
        "url": "/user"
      },
      "public": {
        "url": "/users/:user"
      }
    }
  },
  
  "children": {
  
    "users": {
      "methods": {}
    },
    
    "gists": {
      "methods": {}
    },
    
    "data": {
      "methods": {}
    },
    
    "issues": {
      "methods": {}
    },
    
    "orgs": {
      "methods": {}
    },
    
    "pulls": {
      "methods": {}
    },
    
    "repos": {
      "methods": {
        
        "getAll": {
          "private": {
            "url": "/user/repos?type=all"
          },
          "public": {
            "url": "/users/:user/repos?type=all"
          }
        },
      
        "getPublic": {
          "private": {
            "url": "/user/repos?type=public"
          },
          "public": {
            "url": "/users/:user/repos?type=public",
            "params": {
              "user": {
                "type": "string"
              }
            }
          }
        },
        
        "getPrivate": {
          "private": {
            "url": "/user/repos?type=private"
          }
        },
        
        "getOwnerOf": {
          "private": {
            "url": "/user/repos?type=owner"
          },
          "public": {
            "url": "/users/:user/repos?type=owner"
          }
        },
        
        "getMemberOf": {
          "private": {
            "url": "/user/repos?type=member"
          },
          "public": {
            "url": "/users/:user/repos?type=member"
          }
        }
        
      }
    },
    
    "events": {
      "methods": {}
    }
    
  }
 
}
